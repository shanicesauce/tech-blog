<a href="/homepage"> &larr; Back To Homepage</a>
{{#if posts.length}}
<section>
    <h2>Your Posts</h2>
    <ol>
        {{#each posts as |post|}}
        <ul>
            {{> post-info post}}
            <a href="/dashboard/edit/{{post.id}}" class="edit-link">Edit post</a> 
        </ul>
        {{/each}}
    </ol> 
</section>
{{/if}}

<div>
    <button type="submit"><a href="/dashboard/newpost" class="btn"> New Post </a></button> 
</div>
